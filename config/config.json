    "diagramm-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/diagramm/DiagrammWebPart.js",
          "manifest": "./src/webparts/diagramm/DiagrammWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jsChart":{
      "path": "https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js",
